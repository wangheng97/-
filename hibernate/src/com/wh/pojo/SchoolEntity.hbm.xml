<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.wh.pojo.SchoolEntity" table="school" schema="db05">
        <id name="scid" >
            <column name="scid" sql-type="int(10)"/>
            <generator class="native"/>
        </id>

        <property name="scname">

        <column name="scname" sql-type="varchar(100)" not-null="true" length="100"/>

    </property>
        <property name="address">
        <column name="address" sql-type="varchar(200)" not-null="true" length="200"/>
    </property>
    </class>
</hibernate-mapping>