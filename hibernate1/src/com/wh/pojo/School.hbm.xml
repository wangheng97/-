<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.wh.pojo.School" table="school" schema="db05">
        <id name="scid">
            <column name="scid" sql-type="int(11)"/>
        </id>
        <property name="scname">
            <column name="scname" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>