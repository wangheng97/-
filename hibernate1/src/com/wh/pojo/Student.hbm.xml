<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.wh.pojo.Student" table="student" schema="db05">
        <id name="sid">
            <column name="sid" sql-type="int(11)"/>
        </id>
        <property name="sname">
            <column name="sname" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="date" not-null="true"/>
        </property>
        <property name="gendar">
            <column name="gendar" sql-type="varchar(2)" length="2" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(12)" length="12" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>